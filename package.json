{
  "name": "renderlesskit-react-tailwind-ts-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "postcss": "concurrently yarn:postcss:*",
    "postcss:dev": "postcss -o tailwind-dev.css ./pages/index.css",
    "postcss:prod": "cross-env NODE_ENV=production postcss -o tailwind-prod.css ./pages/index.css"
  },
  "dependencies": {
    "@renderlesskit/react-tailwind": "^0.0.1-alpha.10",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.0.1",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "deepmerge": "^4.2.2",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  }
}
